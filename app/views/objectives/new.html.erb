Add a new objective:

<%= form_for :objective, url: objectives_path do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :deadline %>
    <%= f.date_select :deadline %>
  </p>
  <p>
    <%= f.submit %>
  </p>

<% end %>

<p><%= link_to 'Cancel', objectives_url %></p>
